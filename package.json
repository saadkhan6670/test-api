{
  "name": "test-api",
  "version": "1.0.0",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "prestart": "npm prune && npm install",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor protractor.conf.js",
    "build": "gulp build && gulp bundle",
    "updatedb": "node ./node_modules/geoip-lite/scripts/updatedb.js",
    "update-mmdb": "rm -rf ./data && npm run update-mmdb-v4 && npm run update-mmdb-v6",
    "update-mmdb-v4": "mkdir -p ./data && wget -N http://geolite.maxmind.com/download/geoip/database/GeoLite2-Country.mmdb.gz && gunzip GeoLite2-Country.mmdb.gz && mv GeoLite2-Country.mmdb ./data",
    "update-mmdb-v6": "mkdir -p ./data && wget -N http://geolite.maxmind.com/download/geoip/database/GeoIPv6.dat.gz && gunzip GeoIPv6.dat.gz && mv GeoIPv6.dat ./data",
    "migrate": "NODE_MONGOOSE_MIGRATIONS_CONFIG=./server/config/migrations.js node ./node_modules/mongoose-migrate/bin/migrate --chdir ./server",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "*",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "boom": "^3.2.2",
    "cache-manager": "^2.1.2",
    "cache-manager-mongodb": "^0.1.6",
    "cache-manager-redis": "^0.2.2",
    "composable-middleware": "^0.3.0",
    "compression": "^1.6.2",
    "config": "^1.21.0",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "deasync": "^0.1.7",
    "dotenv": "^2.0.0",
    "ejs": "^2.5.1",
    "eventemitter2": "^2.0.0",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-session": "~1.14.0",
    "express-useragent": "latest",
    "express-winston": "^2.0.0",
    "html-minifier": "^3.0.1",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "maxmind": "^1.1.0",
    "method-override": "~2.3.6",
    "mobile-detect": "^1.3.2",
    "moment": "^2.13.0",
    "mongodb": "^2.1.21",
    "mongoose": "^4.5.2",
    "mongoose-migrate": "^0.2.4",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^1.0.2",
    "mongoose-validator": "^1.2.5",
    "morgan": "~1.7.0",
    "ms": "^0.7.1",
    "passport": "~0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-local": "~1.0.0",
    "passport-twitter": "^1.0.4",
    "passport-twitter-token": "^1.1.1",
    "request": "^2.72.0",
    "sanitizer": "^0.1.3",
    "sync-request": "3.0.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "del": "^2.2.1",
    "fs-readdir-recursive": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-clean": "^0.3.2",
    "gulp-copy": "0.0.2",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-nodemon": "^2.1.0",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.7",
    "gulp-wait": "0.0.2",
    "gulp-watch": "^4.3.8",
    "gulp-zip": "^3.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": true
}
